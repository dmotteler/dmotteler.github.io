<html><head><title>show ShowEvent localStorage</title>
    <script>
        function init() {
            let sevj = localStorage.getItem("showEvent");
            let html = "";
            if (sevj) {
                let sev = JSON.parse(localStorage.getItem("showEvent"));
                let tev = localStorage.getItem("TunersEvents");
                if (tev) {
                    html = `<h1>${tev[sev]['where']}</h1>`
                    html += `<h1>${tev[sev]['when']}</h1>`
                    html += `<h2>${tev[sev]['datndx']}</h2>`
                    for (song of tev[sev]['songlist']) {
                        html += `<h3>${song}</h3>`
                    }
                } else {
                    html += "<h1>TunersEvents localStorage not found.</h1>";
                }
            } else {
                html += "<h1>showEvent localStorage not found.</h1>";
            }
            document.getElementById("place").outerHTML = html;
        }
    </script>
    </head><body onLoad="init()">
        <p id="place"></p>
    </body>
</html>
